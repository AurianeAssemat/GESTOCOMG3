{% extends 'base.html.twig' %}


{% block body %}

<h5> L'habitation de {{habitation.usager.mail}} </h5>



<table class="table table-hover">
  <tr class="table-dark">
    <td>Adresse</td>
    <td>Code Postal</td>
    <td>Ville</td>
    <td>Adresse mail</td>
    <td>Numéro de téléphone</td>

  </tr>
  {% for h in habitation.usager %}
  <tr class="table-warning">
    <td>{{h.adresse}}</td>
    <td>{{h.copos}}</td>
    <td>{{h.ville}}</td>
    <td>{{h.mail}}</td>
    <td>{{h.tel}}</td>
  </tr>




  {% endfor %}
</table>
